{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://ipnityai.com/$1",
      "permanent": true
    },
    {
      "source": "/",
      "destination": "https://ipnityai.com",
      "has": [
        {
          "type": "host",
          "value": "www.ipnityai.com"
        }
      ],
      "permanent": true
    }
  ]
}